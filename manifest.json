{
  "manifest_version": 2,

  "name": "Be Active!",
  "description": "Show's an allarm every hour and show an excercise",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Give me my excercise!"
  },
  "permissions": [
    "activeTab",
    "tabs",
    "alarms",
    "https://ajax.googleapis.com/",
    "http://www.bodybuilding.com/"
  ]
}